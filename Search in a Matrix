class Sol
{
    public static int matSearch(int mat[][], int N, int M, int X)
    {
        int r = 0;
        for(int i=0;i<N;i++)
        {
            for(int j=0;j<M;j++)
            {
                if(mat[i][j]==X)
                {
                    r = 1;
                }
            }
        }
        return r;
    }
}
